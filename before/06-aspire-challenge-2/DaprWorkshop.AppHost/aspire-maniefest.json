{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "pizzaorderservice": {
      "type": "project.v0",
      "path": "../../../after/05-aspire-challenge-1/PizzaOrder/PizzaOrder.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{pizzaorderservice.bindings.http.targetPort}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "pizzaorderservice-dapr": {
      "type": "dapr.v0",
      "dapr": {
        "application": "pizzaorderservice",
        "appId": "pizza-order",
        "daprHttpPort": 3501,
        "resourcesPath": [
          "../../../after/aspire-shared/resources-simple"
        ]
      }
    }
  }
}